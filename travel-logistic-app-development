      $("#nav1").hover(function () {
        $("#nav1").prev().toggleClass("previous");
      });
      $("#nav2").hover(function () {
        $("#nav2").prev().toggleClass("previous");
      });
      $("#nav3").hover(function () {
        $("#nav3").prev().toggleClass("previous");
      });
      $("#nav4").hover(function () {
        $("#nav4").prev().toggleClass("previous");
      });
    </script>
